   "source": [
    "from sklearn.datasets import load_wine\n",
    "data = load_wine()\n",
    "x0 = data.data[:,0]\n",
    "x1 = data.data[:,1]\n",
    "x2 = data.data[:,2]\n",
    "x3 = data.data[:,3]\n",
    "x4 = data.data[:,4]\n",
    "x5 = data.data[:,5]\n",
    "x6 = data.data[:,6]\n",
    "x7 = data.data[:,7]\n",
    "x8 = data.data[:,8]\n",
    "x9 = data.data[:,9]\n",
    "x10 = data.data[:,10]\n",
